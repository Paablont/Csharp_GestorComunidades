<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Csharp_GestorComunidades"
        xmlns:dxsch="http://schemas.devexpress.com/winfx/2008/xaml/scheduling" x:Class="Csharp_GestorComunidades.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="txtTitulo" Content="GESTOR DE COMUNIDADES" HorizontalAlignment="Center" Margin="0,31,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="24"/>
        <Button x:Name="btnManageNBH" Content="Gestionar Comunidad" HorizontalAlignment="Center" Margin="0,126,0,0" VerticalAlignment="Top" Height="46" Width="148"/>
        <Button x:Name="btnCreateNBH" Content="Crear una nueva comunidad" HorizontalAlignment="Center" VerticalAlignment="Center" Height="46" Width="200" Click="OpenNewComunity"/>

    </Grid>
</Window>
